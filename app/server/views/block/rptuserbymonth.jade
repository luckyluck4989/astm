extends ../layout

block content
	#main-content
		.row
			.col-md-12
				.box.box-orange
					.box-title
						h3
							i.icon-file
							| Report Used By Month
						.box-tool
							a(data-action="collapse", href="#")
								i.icon-chevron-up
							a(data-action="close", href="#")
								i.icon-remove
					.box-content
						.row
							.col-md-6(style="width:100px")
								.form-group
									.col-sm-9.col-lg-10.controls
										input#textfield1.form-control(style='width:80px', type='text', name='from', id='from', placeholder='From', tabindex = '1')
							.col-md-6(style="width:100px")
								.form-group
									.col-sm-9.col-lg-10.controls
										input#textfield2.form-control(style='width:80px', type='text', name='to', id='to', placeholder='To', tabindex = '2')
							.col-md-6(style="width:150px")
								.form-group
									.col-sm-9.col-sm-offset-3.col-lg-10.col-lg-offset-2
										button.btn.btn-primary(type='submit', tabindex = '3', id = "btnSearch")
											| Report Search
							.col-md-6(style="width:100px")
								.form-group
									.col-sm-9.col-sm-offset-3.col-lg-10.col-lg-offset-2
										button.btn.btn-primary(style='display:none', type='submit', tabindex = '3', id = "btnExport")
											| Export
						#visualization(style='align:center;width:1028px;height:500px')
block scripts
	script(type='text/javascript', src='//www.google.com/jsapi')
	script(type='text/javascript')
		google.load('visualization', '1', {packages: ['corechart']});
	script(src='http://www.google.com/uds/?file=visualization&v=1&packages=corechart', type='text/javascript')
	script(src='http://www.google.com/uds/api/visualization/1.0/d780f2951a73e815f003b2b487c1d0a3/format+vi,default,corechart.I.js', type='text/javascript')
	script(type='text/javascript')
	link(href='http://ajax.googleapis.com/ajax/static/modules/gviz/1.0/core/tooltip.css', rel='stylesheet', type='text/css')
	script(src='/javascripts/rptuserbymonth.js')
	script(src='/javascripts/rgbcolor.js')
	script(src='/javascripts/canvg.js')
	script(src='/javascripts/libs/base64.js')
	script(src='/javascripts/libs/sprintf.js')
	script(src='/javascripts/jspdf.js')